{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "sui-bundler build -C && cp ./_redirects ./public",
    "co": "sui-mono commit",
    "deploy": "npm run build && firebase deploy",
    "start": "sui-bundler dev",
    "lint": "sui-lint js --fix && sui-lint sass",
    "precommit": "sui-precommit run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@s-ui/mono": "1",
    "@s-ui/precommit": "2",
    "@schibstedspain/sui-bundler": "2",
    "@schibstedspain/sui-lint": "2",
    "cypress": "0.20.0",
    "husky": "0.13.4"
  },
  "dependencies": {
    "@s-ui/react-domain-connector": "1",
    "@schibstedspain/rosetta": "3",
    "babel-polyfill": "6.26.0",
    "firebase": "4.3.0",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-autosize-textarea": "0.4.8",
    "react-dom": "15.5.4",
    "react-icons": "2.2.5",
    "react-loadable": "4.0.4",
    "react-router": "2.8.1",
    "recompose": "0.23.4",
    "rxjs": "5.4.3",
    "styletron-client": "2.5.7",
    "styletron-react": "2.5.8"
  },
  "sui-bundler": {
    "offline": {
      "whitelist": [
        "::all::"
      ]
    },
    "vendor": [
      "babel-polyfill",
      "react",
      "react-dom"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "types": "@s-ui/mono/src/types"
    }
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@schibstedspain/sui-lint/eslintrc.js"
    ]
  },
  "sasslintConfig": "./node_modules/@schibstedspain/sui-lint/sass-lint.yml"
}
